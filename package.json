{
  "name": "itzy",
  "description": "A technology & thought blog",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Ahmed Zetao Yang <vitzys@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZetaoYang/zetaoyang.github.io"
  },
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "gatsby-plugin",
    "blog"
  ],
  "license": "CC BY 4.0",
  "scripts": {
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "build": "gatsby build",
    "build:cdn": "gatsby build --prefix-paths",
    "build:prod": "yarn clean && yarn build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "postbuild": "cp static/CNAME public/",
    "clean": "rm -rf .cache ../public"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "dayjs": "^1.8.21",
    "gatsby": "^2.19.22",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-brotli": "^1.3.1",
    "gatsby-plugin-catch-links": "^2.1.26",
    "gatsby-plugin-feed": "^2.3.27",
    "gatsby-plugin-google-analytics": "2.1.36",
    "gatsby-plugin-layout": "^1.1.22",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-mdx": "^1.0.74",
    "gatsby-plugin-netlify": "^2.1.33",
    "gatsby-plugin-no-sourcemaps": "^2.1.2",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-page-creator": "^2.1.40",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-preact": "^3.1.26",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-remark-autolink-headers": "^2.1.24",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-prismjs": "^3.3.31",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-yaml": "^2.2.25",
    "lodash": "^4.17.15",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "preact": "^10.3.2",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "prettier": "^1.19.1",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.3"
  },
  "peerDependencies": {
    "gatsby": "^2.19.22",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
